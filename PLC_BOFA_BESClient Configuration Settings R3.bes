		<DefaultAction ID="Action1">
			<Description>
				<PreLink>Click </PreLink>
				<Link>here</Link>
				<PostLink> to deploy this action.</PostLink>
			</Description>
			<ActionScript MIMEType="application/x-Fixlet-Windows-Shell"><![CDATA[//Check the individual values of the settings.
if { not (exists settings "_BESClient_RelaySelect_IntervalSeconds" whose(( exist value of it) and (value of it as string as integer >= 43200) ) of client)}
	setting "_BESClient_RelaySelect_IntervalSeconds"="43200" on "{parameter "action issue date" of action}" for client
endif

if {not (exists settings "_BESClient_RelaySelect_MaximumTTLToPing" whose(( exist value of it) and (value of it as string as integer >= 10) ) of client)}
	setting "_BESClient_RelaySelect_MaximumTTLToPing"="10" on "{parameter "action issue date" of action}" for client
endif

if {not (exists settings "_BESClient_Report_MinimumInterval" whose(( exist value of it) and (value of it as string as integer >= 360) ) of client)}
	setting "_BESClient_Report_MinimumInterval"="360" on "{parameter "action issue date" of action}" for client
endif

if {not (exists settings "_BESClient_RelaySelect_MaxRetryIntervalSeconds" whose(( exist value of it) and (value of it as string as integer >= 43200) ) of client)}
	setting "_BESClient_RelaySelect_MaxRetryIntervalSeconds"="43200" on "{parameter "action issue date" of action}" for client
endif

if {not (exists settings "_BESClient_RelaySelect_MinRetryIntervalSeconds" whose(( exist value of it) and (value of it as string as integer >= 3600) ) of client)}
	setting "_BESClient_RelaySelect_MinRetryIntervalSeconds"="3600" on "{parameter "action issue date" of action}" for client
endif

if {not (exists settings "_BESClient_RelaySelect_ResistFailureIntervalSeconds" whose(( exist value of it) and (value of it as string as integer >= 1800) ) of client)}
	setting "_BESClient_RelaySelect_ResistFailureIntervalSeconds"="1800" on "{parameter "action issue date" of action}" for client
endif

if {not (exists settings "_BESClient_Download_Direct" whose(( exist value of it) and (value of it as string as integer = 0) ) of client)}
	setting "_BESClient_Download_Direct"="0" on "{parameter "action issue date" of action}" for client
endif

if {not (exists settings "_BESClient_Log_Days" whose(( exist value of it) and (value of it as string as integer >= 30) ) of client)}
	setting "_BESClient_Log_Days"="30" on "{parameter "action issue date" of action}" for client
endif

if {not (exists settings "_BESClient_Log_MaxSize" whose(( exist value of it) and (value of it as string as integer >= 1024000) ) of client)}
	setting "_BESClient_Log_MaxSize"="1024000" on "{parameter "action issue date" of action}" for client
endif

if {not (exists settings "_BESClient_Resource_InterruptSeconds" whose(( exist value of it) and (value of it as string as integer <= 600) ) of client)}
	setting "_BESClient_Resource_InterruptSeconds"="600" on "{parameter "action issue date" of action}" for client
endif


//Stamp the time this action has last hit the machine
setting "Cust_PLC_Client_ICMP_Setting_applied"="{now}" on "{parameter "action issue date" of action}" for client]]></ActionScript>
