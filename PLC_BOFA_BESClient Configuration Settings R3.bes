		<GroupRelevance JoinByIntersection="true">
			<SearchComponentRelevance Comparison="IsTrue">
				<Relevance><![CDATA[(version of client >= "6.0.0.0") 

AND 

(

   NOT (
    (exists setting "Cust_PLC_Client_ICMP_Setting_excld" whose (value of it as string != "")  of client)
     OR 
    (exists setting "Cust_PLC_Inf_Server_excld" whose (value of it as string != "")  of client)
   )
)

AND 
(    
 
 not (exists settings "_BESClient_RelaySelect_IntervalSeconds" whose(( exist value of it) and (value of it as string as integer >= 43200) ) of client)
 OR not (exists settings "_BESClient_RelaySelect_MaximumTTLToPing" whose(( exist value of it) and (value of it as string as integer >= 10) ) of client)
 OR not (exists settings "_BESClient_Report_MinimumInterval" whose(( exist value of it) and (value of it as string as integer >= 360) ) of client)
 OR not (exists settings "_BESClient_RelaySelect_MaxRetryIntervalSeconds" whose(( exist value of it) and (value of it as string as integer >= 43200) ) of client)
 OR not (exists settings "_BESClient_RelaySelect_MinRetryIntervalSeconds" whose(( exist value of it) and (value of it as string as integer >= 3600) ) of client)
 OR not (exists settings "_BESClient_RelaySelect_ResistFailureIntervalSeconds" whose(( exist value of it) and (value of it as string as integer >= 1800) ) of client)
 OR not (exists settings "_BESClient_Download_Direct" whose(( exist value of it) and (value of it as string as integer = 0) ) of client)
 OR not (exists settings "_BESClient_Log_Days" whose(( exist value of it) and (value of it as string as integer >= 30) ) of client)
 OR not (exists settings "_BESClient_Log_MaxSize" whose(( exist value of it) and (value of it as string as integer >= 1024000) ) of client)
 OR not (exists settings "_BESClient_Resource_InterruptSeconds" whose(( exist value of it) and (value of it as string as integer <= 600) ) of client)

                            
)]]></Relevance>
